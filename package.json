{
  "name": "beaver-di",
  "version": "1.0.4",
  "description": "Dependency injection container for NodeJS",
  "main": "lib/index.js",
  "scripts": {
    "babel": "./node_modules/.bin/babel-node --",
    "start": "nodemon --exec npm run babel --",
    "build": "babel src --out-dir lib --source-maps",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore --fix",
    "test": "mocha -r @babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidodizi/beaver.git"
  },
  "keywords": [
    "di",
    "dic",
    "ioc",
    "dependency injection container",
    "inversion of control container",
    "dependecy injection",
    "inversion of control"
  ],
  "author": "Guido Dizioli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guidodizi/beaver/issues"
  },
  "homepage": "https://github.com/guidodizi/beaver#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "mocha": "^7.0.1",
    "nanoid": "^2.1.11",
    "nodemon": "^2.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "engines": {
    "npm": ">=6.13.4",
    "node": ">=12.14.1"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "get-parameter-names": "^0.3.0",
    "lodash": "^4.17.15"
  }
}
